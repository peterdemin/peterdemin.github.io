upstream reader {
    server unix:/run/reader/reader.sock;
}

server {
    server_name r.demin.dev;
    listen 100.121.131.14:80;

    location / {
      proxy_pass http://reader;
      proxy_http_version 1.0;
      proxy_set_header Connection "close";
      proxy_set_header Host $http_host;
    }
}
